{
  "name": "predic",
  "displayName": "Predic",
  "description": "Your offline AI powered code completion extension",
  "publisher": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sayemahamed/Predic.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run clean && webpack --mode production",
    "build:dev": "npm run clean && webpack --mode development",
    "watch": "npm run clean && webpack --watch --mode development --stats minimal",
    "clean": "rimraf dist",
    "lint": "eslint src --ext ts",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "rimraf": "^5.0.5",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.1"
  }
}