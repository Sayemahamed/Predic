{
  "name": "predic",
  "displayName": "Predic",
  "description": "Your offline AI powered code completion extension",
  "publisher": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sayemahamed/Predic.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:css"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Predic",
      "properties": {
        "predic.pythonPath": {
          "type": "string",
          "default": "",
          "description": "The absolute path to the Python executable (e.g., from your virtual environment). If empty, the extension will try to use the Python command in the system's PATH."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "build": "node ./build.mjs",
    "watch": "node ./build.mjs --watch",
    "watch:build": "npm run watch",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "fs-extra": "^11.3.0"
  },
  "private": true,
  "dependencies": {
    "@huggingface/transformers": "^3.5.2",
    "@xenova/transformers": "^2.17.1",
    "onnxruntime-node": "^1.22.0-rev",
    "sharp": "^0.33.4"
  }
}
